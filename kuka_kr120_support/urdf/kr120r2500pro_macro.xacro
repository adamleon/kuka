<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:property name="DEG2RAD" value="0.017453292519943295"/>
    <xacro:property name="PI" value="3.141592653589793"/>

    <xacro:macro name="kuka_kr120r2500pro" params="prefix">
        <!-- LINKS -->
            <!-- base link -->
            <link name="${prefix}base_link">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/base_link.dae" />
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/base_link.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 1 (A1) -->
            <link name="${prefix}link_1">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <!-- main link body -->
                <visual>
                    <origin xyz="0 0 -0.2156" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_1.dae" />
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="0 0 -0.2156" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_1.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 2 -->
            <link name="${prefix}link_2">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="-0.35 0 -0.675" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_2.dae"/>
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="-0.35 0 -0.675" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_2.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 3 -->
            <link name="${prefix}link_3">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="-0.35 0 -1.825" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_3.dae" />
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="-0.35 0 -1.825" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_3.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 4 -->
            <link name="${prefix}link_4">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="-1.094 0 -1.784" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_4.dae" />
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="-1.094 0 -1.784" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_4.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 5 -->
            <link name="${prefix}link_5">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="-1.350 0 -1.784" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_5.dae" />
                    </geometry>
                </visual>
                <collision>
                    <origin xyz="-1.350 0 -1.784" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_5.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- link 6 -->
            <link name="${prefix}link_6">
                <inertial>
                    <origin xyz="0 0 0" rpy="0 0 0"/>
                    <mass value="2"/>
                    <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
                </inertial>
                <visual>
                    <origin xyz="-1.565 0 -1.784" rpy="0 0 0"/>
                    <!--<origin xyz="-1.538 0 -1.784" rpy="0 0 0"/>-->
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/visual/link_6.dae" />
                    </geometry>
                    <material name="gray">
                        <color rgba="0.5 0.5 0.5 1"/>
                    </material>
                </visual>
                <collision>
                    <origin xyz="-1.538 0 -1.784" rpy="0 0 0"/>
                    <geometry>
                        <mesh filename="package://kuka_kr120_support/meshes/r2500pro/collision/link_6.stl" scale="0.001 0.001 0.001" />
                    </geometry>
                </collision>
            </link>
            <!-- tool 0 -->
            <link name="${prefix}tool0"/>
        <!-- END LINKS -->

        <!-- JOINTS -->
            <!-- joint 1 (A1) -->
            <joint name="${prefix}joint_1" type="revolute">
                <origin xyz="0 0 0.2156" rpy="0 0 0"/>
                <parent link="${prefix}base_link"/>
                <child link="${prefix}link_1"/>
                <axis xyz="0 0 -1"/>
                <limit effort="0" lower="${-DEG2RAD * 185}" upper="${DEG2RAD * 185}" velocity="${DEG2RAD * 156}"/>
            </joint>
            <!-- joint 2 (A2) -->
            <joint name="${prefix}joint_2" type="revolute">
                <origin xyz="0.35 0 0.4594" rpy="0 ${PI/2} 0"/>
                <parent link="${prefix}link_1"/>
                <child link="${prefix}link_2"/>
                <axis xyz="0 1 0"/>
                <limit effort="0" lower="${-DEG2RAD * 155}" upper="${DEG2RAD * 35}" velocity="${DEG2RAD * 156}"/>
            </joint>
            <!-- joint 3 (A3) -->
            <joint name="${prefix}joint_3" type="revolute">
                <origin xyz="0 0 1.150" rpy="0 ${-PI/2} 0"/>
                <parent link="${prefix}link_2"/>
                <child link="${prefix}link_3"/>
                <axis xyz="0 1 0"/>
                <limit effort="0" lower="${-DEG2RAD * 130}" upper="${DEG2RAD * 154}" velocity="${DEG2RAD * 156}"/>
            </joint>
            <!-- joint 4 (A4) -->
            <joint name="${prefix}joint_4" type="revolute">
                <origin xyz="0.744 0 -0.041" rpy="0 0 0"/>
                <parent link="${prefix}link_3"/>
                <child link="${prefix}link_4"/>
                <axis xyz="-1 0 0"/>
                <limit effort="0" lower="${-DEG2RAD * 350}" upper="${DEG2RAD * 350}" velocity="${DEG2RAD * 330}"/>
            </joint>
            <!-- joint 5 (A5) -->
            <joint name="${prefix}joint_5" type="revolute">
                <origin xyz="0.256 0 0" rpy="0 0 0"/>
                <parent link="${prefix}link_4"/>
                <child link="${prefix}link_5"/>
                <axis xyz="0 1 0"/>
                <limit effort="0" lower="${-DEG2RAD * 130}" upper="${DEG2RAD * 130}" velocity="${DEG2RAD * 330}"/>
            </joint>
            <!-- joint 6 (A6) -->
            <joint name="${prefix}joint_6" type="revolute">
                <!--<origin xyz="0.188 0 0" rpy="0 0 0"/>-->
                <origin xyz="0.215 0 0" rpy="0 0 0"/>
                <parent link="${prefix}link_5"/>
                <child link="${prefix}link_6"/>
                <axis xyz="-1 0 0"/>
                <limit effort="0" lower="${-DEG2RAD * 350}" upper="${DEG2RAD * 350}" velocity="${DEG2RAD * 615}"/>
            </joint>
            <!-- tool frame - fixed frame -->
            <joint name="${prefix}joint_6-tool0" type="fixed">
                <parent link="${prefix}link_6"/>
                <child link="${prefix}tool0"/>
                <origin xyz="0 0 0" rpy="0 ${PI/2} 0"/>
            </joint>
        <!-- END JOINTS -->


    </xacro:macro>
</robot>


